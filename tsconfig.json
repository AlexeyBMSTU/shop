{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "baseUrl": "src",
      "paths": {
        "@/*": ["*"]
      },
      "jsx": "react"
    },
    "include": ["src/**/*", "images.d.ts"]
  }